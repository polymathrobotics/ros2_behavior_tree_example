<root BTCPP_format="4">
    <BehaviorTree ID="MainTree">
        <Fallback>
            <ReactiveSequence>
                <PingReceivedExecutorNode force_ping="{ping_start}" last_ping_id="{ping_id}"/>
                <Repeat num_cycles="{num_publish}">
                    <PongNode num_pongs="1" last_pong_id="{pong_id}"/>
                </Repeat>
            </ReactiveSequence>
            <PublishStatusNode message="No ping received" print_ping_pong="True" ping_id="{ping_id}" pong_id="{pong_id}"/>
        </Fallback>
    </BehaviorTree>
</root>